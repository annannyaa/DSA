int getSum(BinaryTreeNode<int>* root) {
    // Write your code here
    int sum=root->data;
    if(root->left==NULL && root->right==NULL){
        return sum;
    }
    if(root->left==NULL){
        sum=sum+getSum(root->right);
        return sum;
    }
    if(root->right==NULL){
        sum=sum+getSum(root->left);
        return sum;
    }
    
sum=sum+getSum(root->left);
sum=sum+getSum(root->right);
    return sum;
}
